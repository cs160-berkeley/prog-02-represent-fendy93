<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Information based on"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:padding="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Location: "
        android:id="@+id/textView3"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:paddingLeft="10dp"
        android:paddingBottom="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Current Location"
        android:id="@+id/curr"
        android:layout_below="@+id/textView"
        android:layout_toEndOf="@+id/textView3"
        android:paddingBottom="10dp"
        android:textSize="19dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/textView18"
        android:background="#acacac"
        android:layout_above="@+id/tabHost_curr"
        android:layout_alignParentStart="true" />

    <TabHost
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/tabHost_curr"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"></TabWidget>

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/tab_senator_curr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="left|center_vertical">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:layout_width="100dp"
                            android:layout_height="90dp"
                            android:id="@+id/bm_img"
                            android:src="@mipmap/bmikulski"
                            android:scaleType="fitXY"
                            android:background="#FFEAEAEA"
                            android:layout_gravity="center_vertical" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Barbara Mikulski"
                                android:id="@+id/bm_name"
                                android:paddingBottom="5dp"
                                android:textSize="13dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Democrat"
                                android:id="@+id/bm_party"
                                android:paddingBottom="5dp"
                                android:textColor="#0008ff" />

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/bm_web"
                                    android:src="@mipmap/com_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitCenter" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/bm_contact"
                                    android:src="@mipmap/mails_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitXY" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="More Info"
                                android:id="@+id/bm_text"
                                android:paddingBottom="5dp"
                                android:textColor="#3a3f9d" />

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView8"
                                android:background="#acacac" />

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:id="@+id/imageView3"
                                android:layout_gravity="center_horizontal"
                                android:src="@mipmap/tweet_logo"
                                android:scaleType="center" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView9"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@barbaramikulski: Happy Valentines Day!"
                                android:id="@+id/textView10"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:id="@+id/textView11"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@barbaramikulski: Happy New Year!"
                                android:id="@+id/textView12"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView13"
                                android:background="#acacac" />
                        </LinearLayout>

                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:id="@+id/textView14"
                        android:background="#DADADA" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <ImageButton
                            android:layout_width="100dp"
                            android:layout_height="90dp"
                            android:id="@+id/bb_img"
                            android:scaleType="fitXY"
                            android:background="#FFEAEAEA"
                            android:layout_gravity="center_vertical"
                            android:src="@mipmap/dfeinstein" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Barbara Boxer"
                                android:id="@+id/bboxer_name"
                                android:paddingBottom="5dp"
                                android:textSize="13dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Democrat"
                                android:id="@+id/bboxer_party"
                                android:paddingBottom="5dp"
                                android:textColor="#0008ff" />

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" >

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/bb_web"
                                    android:src="@mipmap/com_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitCenter" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/bb_contact"
                                    android:src="@mipmap/mails_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitXY" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="More Info"
                                android:id="@+id/bb_text"
                                android:paddingBottom="5dp"
                                android:textColor="#3a3f9d" />
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView25"
                                android:background="#acacac" />

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:id="@+id/imageView6"
                                android:layout_gravity="center_horizontal"
                                android:src="@mipmap/tweet_logo"
                                android:scaleType="center" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView19"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@diannefeinstein: Happy Valentines Day!"
                                android:id="@+id/textView20"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:id="@+id/textView21"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@dainnefeinstein: Happy New Year!"
                                android:id="@+id/textView22"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView23"
                                android:background="#acacac" />
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:id="@+id/textView24"
                        android:background="#DADADA" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/tab_rep_curr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="left|center_vertical">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <ImageButton
                            android:layout_width="100dp"
                            android:layout_height="90dp"
                            android:id="@+id/blee_img"
                            android:src="@mipmap/blee"
                            android:scaleType="fitXY"
                            android:background="#FFEAEAEA"
                            android:layout_gravity="center_vertical" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Barbara lee"
                                android:id="@+id/blee_name"
                                android:paddingBottom="5dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="Democrat"
                                android:id="@+id/blee_party"
                                android:paddingBottom="5dp"
                                android:textColor="#0008ff" />

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" >

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/blee_web"
                                    android:src="@mipmap/com_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitCenter" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/blee_email"
                                    android:src="@mipmap/mails_logo"
                                    android:paddingBottom="10dp"
                                    android:scaleType="fitXY" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="More Info"
                                android:id="@+id/blee_text"
                                android:paddingBottom="5dp"
                                android:textColor="#3a3f9d" />
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView30"
                                android:background="#acacac" />

                            <ImageView
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:id="@+id/imageView9"
                                android:layout_gravity="center_horizontal"
                                android:src="@mipmap/tweet_logo"
                                android:scaleType="center" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView29"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@barbaralee: Happy Valentines Day!"
                                android:id="@+id/textView31"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:id="@+id/textView32"
                                android:background="#acacac" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="\@barbaralee: Happy New Year!"
                                android:id="@+id/textView33"
                                android:layout_gravity="center_horizontal"
                                android:padding="5dp"
                                android:background="#ffffff" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="2dp"
                                android:id="@+id/textView34"
                                android:background="#acacac" />
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:id="@+id/textView35"
                        android:background="#DADADA" />

                </LinearLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>
</RelativeLayout>